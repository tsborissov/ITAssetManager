@model AssetCollectServiceModel

@{
    ViewBag.Title = "Collect Asset";
}

<div class="row">
    <div class="col-sm-12 offset-lg-2 col-lg-8 offset-xl-3 col-xl-6">
        <h2 class="heading-margin text-center">@ViewBag.Title</h2>
        <form method="post">
            <div class="form-group">
                <label asp-for="Model"></label>
                <input asp-for="Model" class="form-control" readonly />
            </div>
            <div class="form-group">
                <label asp-for="SerialNr"></label>
                <input asp-for="SerialNr" class="form-control" readonly />
            </div>
            <div class="form-group">
                <label asp-for="InventoryNr"></label>
                <input asp-for="InventoryNr" class="form-control" readonly />
            </div>
            <div class="form-group">
                <label asp-for="AssignDate"></label>
                <input asp-for="AssignDate" class="form-control" readonly />
            </div>
            <div class="form-group">
                <label asp-for="ReturnDate"></label>
                <input asp-for="ReturnDate" class="form-control" />
                <span asp-validation-for="ReturnDate" class="small text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="UserName"></label>
                <input asp-for="UserName" class="form-control" readonly />
            </div>
            <div class="form-group">
                <input asp-for="UserId" class="form-control" readonly hidden />
                <input asp-for="Id" class="form-control" readonly hidden />
            </div>
            <partial name="_BackSaveButtonsToAllPartial" model="@Model" />
        </form>
    </div>
</div>